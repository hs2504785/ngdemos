/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@libs/guitardemo": [
        "dist/libs/guitardemo/libs-guitardemo",
        "dist/libs/guitardemo"
      ],
      "@libs/idb-demo": [
        "dist/libs/idb-demo/libs-idb-demo",
        "dist/libs/idb-demo"
      ],
      "@libs/shared": ["dist/libs/shared/libs-shared", "dist/libs/shared"],
      "blog": ["dist/blog/blog", "dist/blog"],
      "blogdemo": ["dist/blogdemo/blogdemo", "dist/blogdemo"],
      "chat": ["dist/chat/chat", "dist/chat"],
      "crud": ["dist/crud/crud", "dist/crud"],
      "data-source": ["dist/data-source/data-source", "dist/data-source"],
      "ebook": ["dist/ebook/ebook", "dist/ebook"],
      "github-repo": ["dist/github-repo/github-repo", "dist/github-repo"],
      "rxdb": ["dist/rxdb/rxdb", "dist/rxdb"],
      "test": ["dist/test/test", "dist/test"],
      "todo": ["dist/todo/todo", "dist/todo"],
      "workerdemo": ["dist/workerdemo/workerdemo", "dist/workerdemo"]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "module": "es2020",
    "lib": ["es2018", "dom"],
    "useDefineForClassFields": false
  },
  "angularCompilerOptions": {
    "strictTemplates": false,
    "enableI18nLegacyMessageIdFormat": false
  }
}
