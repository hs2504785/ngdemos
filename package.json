{
  "name": "ngdemos",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prettier": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "start": "ng serve -o --hmr",
    "mailapp": "npm run server && ng serve -o --hmr --project=mailapp",
    "demoapp": "ng serve -o --hmr --project=demoapp",
    "build": "npm run build-lib && ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lib-watch": "ng build data-source --watch",
    "build-lib": "ng build data-source",
    "watch": "ng build --watch --configuration development",
    "generate": "node ./server/generate.js > ./server/database.json",
    "server": "json-server --watch ./server/database.json  --id=_id",
    "dev": "concurrently \"npm run server\" \"npm run start\"",
    "sme-build": "ng build --source-map",
    "sme": "npm run sme-build && source-map-explorer dist/**/*.js",
    "wba-build": "ng build --stats-json",
    "wba": "npm run wba-build && webpack-bundle-analyzer dist/**/stats.json",
    "wba-build-app": "ng build --project=i18napp --stats-json",
    "wbaapp": "npm run wba-build-app && webpack-bundle-analyzer dist/i18napp/stats.json",
    "purifyCSS": "gulp --gulpfile tools/gulpfile.js purifyCSS",
    "purgecss": "gulp --gulpfile tools/gulpfile.js purgecss",
    "perf": "env NG_BUILD_MANGLE=false NG_BUILD_MINIFY=false NG_BUILD_BEAUTIFY=true ng serve --prod",
    "perf:report": "lighthouse --throttling.cpuSlowdownMultiplier=2.4 --output-path=./dist/lighthouse-report.html http://localhost:4200 --budget-path=./tools/budget.json --view",
    "predemodeploy": "ng build --project=demoapp --base-href \"/ngdemos/demoapp/\"",
    "demodeploy": "ngh --dir=dist --message=\"My First deploy\"",
    "prei18ndeploy": "ng build --project=i18napp --base-href \"/ngdemos/i18napp/\"",
    "i18ndeploy": "ngh --dir=dist --message=\"My First deploy\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.4",
    "@angular/cdk": "^12.0.4",
    "@angular/common": "~12.0.4",
    "@angular/compiler": "~12.0.4",
    "@angular/core": "~12.0.4",
    "@angular/fire": "^6.1.5",
    "@angular/forms": "~12.0.4",
    "@angular/material": "^12.0.4",
    "@angular/platform-browser": "~12.0.4",
    "@angular/platform-browser-dynamic": "~12.0.4",
    "@angular/router": "~12.0.4",
    "@angular/service-worker": "~12.0.4",
    "@ngrx/data": "^12.1.0",
    "@ngrx/effects": "^12.1.0",
    "@ngrx/entity": "^12.1.0",
    "@ngrx/router-store": "^12.1.0",
    "@ngrx/store": "^12.1.0",
    "@ngrx/store-devtools": "^12.1.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "animate.css": "^4.1.1",
    "bootstrap": "^4.6.0",
    "bootswatch": "^4.6.0",
    "firebase": "^7.0 || ^8.0",
    "jquery": "^3.6.0",
    "ng2-nvd3": "^2.0.0",
    "rxjs": "6.6.7",
    "spectrum-colorpicker": "^1.8.1",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.4",
    "@angular/cli": "~12.0.4",
    "@angular/compiler-cli": "~12.0.4",
    "@ngrx/schematics": "^12.1.0",
    "@schuchard/prettier": "^5.0.0",
    "@types/faker": "^5.5.5",
    "@types/jasmine": "~3.7.4",
    "@types/node": "^15.6.1",
    "codelyzer": "^6.0.2",
    "concurrently": "^6.2.0",
    "faker": "^5.5.3",
    "gulp": "^4.0.2",
    "gulp-purgecss": "^4.0.3",
    "gulp-purifycss": "^0.2.0",
    "husky": "6.0.0",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~7.0.0",
    "json-server": "^0.16.3",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.6.0",
    "lint-staged": "11.0.0",
    "ng-packagr": "^12.0.0",
    "prettier": "2.3.0",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.5.2",
    "ts-node": "~10.0.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "~4.2.4",
    "webpack-bundle-analyzer": "^4.4.2",
    "@angular-devkit/architect": ">= 0.900 < 0.1300",
    "firebase-tools": "^8.0.0 || ^9.0.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "open": "^7.0.3",
    "jsonc-parser": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,less,md,ts,html,component.html}": [
      "prettier --write"
    ]
  }
}
